            <li><a href=""><i class="icon-facebook" title="Compartilhe no Facebook"></i>1.5 K</a></li>
            <li><a href=""><i class="icon-twitter" title="Compartilhe no Twitter"></i>900</a></li>
            <li><a href=""><i class="icon-linkedin" title="Compartilhe no LinkedIn"></i>25</a></li>
          </ul>
          <ul class="onsite">
            <li><a href=""><i class="icon-comments" title="Faça um comentário"></i>12</a></li>
            <li><a href=""><i class="icon-email" title="Envie por e-mail"></i></a></li>
          </ul>
        </nav>
        <% for(var i=0; i < noticia.autor.length; i++){ %>
        <address class="author">
          <figure class="avatar"><img src="<%= noticia.autor[i].avatar %>" alt=""></figure>
          <p class="name"><%= noticia.autor[i].nome %></p>
          <p class="url"><a href="mailto:<%= noticia.autor[i].email %>"><%= noticia.autor[i].email %></a></p>
          <p class="desc">Enviada especial à São Paulo</p>
          <p class="dateTime">
            <time itemprop="dateCreated"><%- new Date(noticia.materia.dataPublicacao).toLocaleDateString() %></time>
            <br>Atualizado às
            <time itemprop="dateModified"><%- new Date(noticia.materia.dataPublicacao).toLocaleTimeString() %></time>
          </p>
        </address>
        <% } %>
      </section>
